{
	"ci": {
		"collect": {
			"url": [
				"http://localhost/hair-style/how-to-style-baby-bangs-100-chic-for-a-night-out/",
				"http://localhost/search-results/",
				"http://localhost/hair-style/20-best-box-braids-bob-styles-to-try-in-2020/"
			],
			"staticDistDir": "packages/web/public"
		},
		"assert": {
			"assertions": {
				"categories:accessibility": ["warn", {
					"minScore": 0.85
				}],
				"categories:best-practices": ["warn", {
					"minScore": 0.85
				}],
				"categories:performance": ["warn", {
					"minScore": 0.85
				}],
				"categories:seo": ["warn", {
					"minScore": 1.0
				}],
				"dom-size": ["warn", {
					"maxNumericValue": 30000
				}],
				"first-contentful-paint": ["warn", {
					"maxNumericValue": 4000
				}],
				"interactive": ["warn", {
					"maxNumericValue": 5000
				}],
				"is-crawlable": false,
				"errors-in-console": ["error", {
					"maxLength": 4
				}],
				"unused-css-rules": "off",
				"uses-rel-preconnect": "off",
				"uses-text-compression": "off",
				"color-contrast": "off",
				"link-name": "off",
				"unused-javascript": "off",
				"uses-responsive-images": "off",
				"uses-passive-event-listeners": "off"
			}
		},
		"upload": {
			"uploadUrlMap": true,
			"target": "temporary-public-storage"
		}
	}
}
